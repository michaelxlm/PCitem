/**index.wxss**/
.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}
page{
    height:100%;
    color:#000;
    background-color:#efefef;
}
.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

.couponView, .couponModel {
    width: 690rpx;
    margin: 4% auto;
    height: 165px;
    position: relative;
}

.couponView .couponLeft, .couponModel .couponLeft {
    width: 40rpx;
    margin:72rpx 22rpx 0 48rpx;
    height: 200rpx;
}

.couponView .inline, .couponModel .inline {
    display: inline-block;
}

.couponView .couponRight, .couponModel .couponRight {
    width: 564rpx;
    margin: 14rpx 0 14rpx 14rpx;
    vertical-align: top;
}

.couponView text {
    display: inline-block;
    font-size: 24rpx;
    line-height: 72rpx;
    vertical-align: top;
}
.couponView .storeName{
    font-size:36rpx;
    margin-left:6rpx;
}
.couponView .marginTop{
    margin-top:6rpx;
}
.couponView .userRule {
    float: right;
    color: #aaa;
    margin-right:15rpx;
}

.couponView .textVal {
    width:338rpx;
    vertical-align:top;
    margin-left:12rpx;

}

.couponView .textVal text {
    width: 290rpx;
    vertical-align: top;
}

.couponView button {
    border-radius:46rpx;
    color:#fff;
    background-color:#fb8643;
    font-size:30rpx;
    width:294rpx;
    margin-top:10rpx;
    line-height:2.2;
    font-weight:600;
letter-spacing: 4rpx;
}

.couponView .parkingImg {
    width: 188rpx;
    vertical-align:top;
}
.couponView .couponIcon {
    width: 190rpx;
}
.couponView .icon {
    width:28rpx;
    margin:12px 8rpx;
    vertical-align:middle;
}

.couponView .buttonImg {
    width: 40rpx;
    vertical-align: middle;
    float:left;
    margin-top:14rpx;
}

.couponModel {
    font-size: 22rpx;
    margin-top: 45%;
    margin-bottom: 15%;
    line-height: 44rpx;
}

.couponModel text {
    display: inline-block;
    width: 446rpx;
    vertical-align: top;
    font-size:21rpx;
}

.couponModel .title {
    font-size:36rpx;
    margin-top:14rpx;
}

.couponModel .orange {
    font-size: 32rpx;
    color: #EAC645;
}

.close {
    width: 90rpx;
    margin: 0 auto;
    display: block;
}

.modelClass {
    background-color: rgba(0, 0, 0, 0.8);
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 99999;
}

.couponView .imgBlackGround,.modelClass .imgBlackGround{
    width: 100%;
    height: 350rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}
.noList{
    text-align:center;
    height:200rpx;
    line-height:200rpx;
    color:#bbb;

}
.model{
    position:absolute;
    top:40%;
    z-index:9999999;
    font-size:28rpx;
    line-height:48rpx;
    background:#fff;
    text-align:center;
    left:20%;
    right:20%;
    border-radius:10rpx;
}
.model .modelTitle{
    line-height:60rpx;
    font-size:33rpx;
    padding-top:12rpx;
}
.modelBody{
    padding:10rpx 20rpx;
}
.mask{
    position: absolute;
    top:0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 999999;
}
